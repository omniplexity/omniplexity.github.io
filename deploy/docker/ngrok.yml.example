version: "3"
agent:
  authtoken: "YOUR_NGROK_AUTHTOKEN"
endpoints:
  - name: omniai-api
    # Leave url empty to get a random HTTPS address (ngrok-free.app)
    # url: http://  # uncomment to force an HTTP endpoint
    upstream:
      url: http://api:8787
    traffic_policy:
      on_http_request:
        - actions:
            - type: add-headers
              config:
                headers:
                  x-origin-secret: "YOUR_ORIGIN_LOCK_SECRET"
